package com.pos.metadataservice.model.db;

import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "cdv_metadata_field_info")
public class MetaDataFieldDetails {

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name = "SL")
	private Integer sl;

	@Column(name = "FCBName", length = 64)
	private String fcbName;

	@Column(name = "Table_Name", length = 64)
	private String tableName;

	@Column(name = "DB_Column_Name", length = 64)
	private String dbColumnName;

	@Column(name = "PSX_Data_Type", length = 64)
	private String psxDataType;

	@Column(name = "Display_Column_Name", columnDefinition = "TEXT")
	private String displayColumnName;

	@Column(name = "Dedupe_Type", length = 64)
	private String dedupeType;

	@Column(name = "Is_To_Participate_In_Dedupe", length = 32)
	private String isToParticipateInDedupe;

	@Column(name = "Domain_Values", columnDefinition = "TEXT")
	private String domainValues;

	@Column(name = "NotNull", length = 32)
	private String notNull;

	@Column(name = "ValidationExpression", columnDefinition = "TEXT")
	private String validationExpression;

}
